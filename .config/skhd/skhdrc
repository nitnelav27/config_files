# web browser
cmd + alt - w : /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --single-instance

# Terminal
cmd + alt - return : /Applications/iTerm.app/Contents/MacOS/iTerm2

# Open Ranger
cmd + alt - r : /Applications/iTerm.app/Contents/MacOS/iTerm2 -e ranger ~

# Scripts to execute
cmd + alt - p : /Users/vsvh/.config/scripts/script -single-instance

# Toggle cam
cmd + alt - c : /Users/vsvh/.config/scripts/ftwebcam

# Emacs
cmd + alt - e: /opt/homebrew/bin/emacsclient -n -c -a ''
